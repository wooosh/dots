# fvwm
cp -rT ~/.fvwm ./.fvwm

# awesomewm
mkdir -p .config/awesome
cp -rT ~/.config/awesome .config/awesome

# cmus
mkdir -p .config/cmus
cp ~/.config/cmus/*.theme  .config/cmus/
cp ~/.config/cmus/autosave .config/cmus/

# .Xresources
cp ~/.Xresources ./
mkdir -p .config
cp ~/.config/urxvtpixmap.png .config/

# fetch
cp ~/fetch fetch

